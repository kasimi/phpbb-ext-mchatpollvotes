{
    "name": "kasimi/mchatpollvotes",
    "type": "phpbb-extension",
    "description": "Adds a notification message to mChat when a user casts a vote in a poll.",
    "homepage": "https://kasimi.net",
    "version": "1.0.2",
    "time": "2023-02-16",
    "keywords": ["mchat", "addon", "notification", "poll", "vote"],
    "license": "GPL-2.0-only",
    "authors": [
		{
			"name": "kasimi",
			"email": "mail@kasimi.net",
			"homepage": "https://kasimi.net",
			"role": "Extension Developer"
		}
	],
    "require": {
        "php": ">=5.4.7",
        "composer/installers": "~1.0"
    },
    "extra": {
        "display-name": "mChat Poll Votes Addon",
        "soft-require": {
            "phpbb/phpbb": ">=3.2.4"
        },
		"version-check": {
			"host": "kasimi.github.io",
			"directory": "/phpbb_versions",
			"filename": "mchatpollvotes_version.json",
			"ssl": true
        }
    }
}
